    // Google Tag Manager
  body.profile-page.sidebar-collapse
    nav.navbar.navbar-expand-lg.nav-down.navbar-absolute.navbar-transparent(color-on-scroll='500')
      .container
        .navbar-translate
          a.navbar-brand(href='./custHome',  data-placement='bottom')
            i.fa.fa-home
            | Home
          button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navigation', aria-controls='navigation-index', aria-expanded='false', aria-label='Toggle navigation')
            span.navbar-toggler-bar.bar1
            span.navbar-toggler-bar.bar2
            span.navbar-toggler-bar.bar3
        .collapse.navbar-collapse(data-nav-image='/assets/paperkit/assets/img/blurred-image-1.jpg', data-color='orange')
          ul.navbar-nav.ml-auto
            li.dropdown.nav-item
              each customer in customers
                a.navbar-brand(href='./custProfile', data-placement='bottom')
                  i.fa.fa-user
                  = customer.cust_fname+" "+customer.cust_lname
                a.navbar-brand(href='', data-placement='bottom')
                  i.fa.fa-sign-out
                  | Logout
                .dropdown-menu.dropdown-menu-right.dropdown-danger
                  a.dropdown-item(href='../index.html')
                    | All Components
                  a.dropdown-item(href='../docs/1.0/getting-started/introduction.html')
                    | Documentation
    // End Navbar
    .page-header.page-header-small(style="background-image: url('/assets/paperkit/assets/img/spabg.jpg');")
      .filter
    .wrapper
      each customer in customers
        .profile-content.section
          .container
            .row
              .profile-picture
                .fileinput.fileinput-new(data-provides='fileinput')
                  .fileinput-new(style="margin-left: 2em")
                    img(src='/assets/paperkit/assets/img/nopicture.jpg', alt='...')
                  .name
                    h4.title.text-center
                      = customer.cust_fname+" "+customer.cust_mname+" "+customer.cust_lname
            .row
              .col-md-6.ml-auto.mr-auto.text-center
                button.btn.btn-outline-info.btn-round(data-toggle='modal', data-target='#loyaltyDetails')
                  i.fa.fa-eye
                  |  View Loyalty Membership Details
            br
            .nav-tabs-navigation
              .nav-tabs-wrapper
                ul.nav.nav-tabs(role='tablist')
                  li.nav-item
                    a.nav-link.active(data-toggle='tab', href='#follows', role='tab') Reservations
                  li.nav-item
                    a.nav-link(data-toggle='tab', href='#following', role='tab') Following
            // Tab panes
            .tab-content.following
              #follows.tab-pane.active(role='tabpanel')
                h3.text-muted Reservation
                br
                button.btn.btn-info.btn-round Book Reservation
              #following.tab-pane.text-center(role='tabpanel')
                h3.text-muted Not following anyone yet :(
                button.btn.btn-warning.btn-round Find artists
    footer.footer
      .container
        .row
          nav.footer-nav
          .credits.ml-auto
            span.copyright
              | Â©
              script.
                document.write(new Date().getFullYear())
              | , made with 
              i.fa.fa-heart.heart
              |  by Team MBAY
