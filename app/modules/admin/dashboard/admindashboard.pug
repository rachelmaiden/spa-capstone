  script(type='text/javascript').
    $(document).ready(function(){
      $('.logout').click(function(){
            swal({
        title: "Are you sure?",
        text: "Do you want to Log Out?",
        icon: "warning",
        buttons: true,
        dangerMode: true,
      })
      .then((willDelete) => {
        if (willDelete) {
          $.post('/admin/admin/logout')
          location.reload()
        } 
      });
      })
    demo.initOverviewDashboard();
    demo.initCirclePercentage();
    });
