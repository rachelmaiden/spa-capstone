  script.
    function calculate(){
      var totalPrice = $('.totalPrice').val()
      var currentPoints = $('.currentPoints').val()
      var pointsTotal = 0;
      var priceTotal = 0;
      var newPrice
      var newPoints

      $(".checkboxPayment:checked").each(function(){
        var id = $(this).val()
        var newhalaga = $(`.newhalaga[id='${id}']`).val()
        var newPointsEq = $(`.newPointsEq[id='${id}']`).val()

        console.log('NEW HALAGA', newhalaga)
        priceTotal = eval(newhalaga+"+"+priceTotal)
        console.log('PRICE TOTAL',priceTotal)
        NewPrice = parseInt(totalPrice) - parseInt(priceTotal)
        pointsTotal = eval(newPointsEq+'+'+pointsTotal)
        NewPoints = parseInt(currentPoints) - parseInt(pointsTotal)
      })
        $('.newPoints').val(NewPoints)
        console.log('NEW PRICE',NewPrice)
        $('.amountNeeded').val(NewPrice)
        $('#AmountNeeded').text('₱'+NewPrice+'.00')

    }
  script.
    $(document).ready(function(){
      $(document).on('change.bootstrapSwitch','.checkboxPayment',function(e){
        var id = $(this).val()
        var quantity = $(`.quantityTotal[id='${id}']`).val()
        var name = $(`.servname[id='${id}']`).val()
        var price = $(`.halaga[id='${id}']`).val()
        var points = $(`.eqTotal[id='${id}']`).val()
        if(e.target.checked==true)
        {
          if(quantity ==1)
          {
            var pricePerServ = parseInt(price) / parseInt(quantity)
            var computed_price = parseInt(quantity) * parseInt(pricePerServ)
            $(`.newhalaga[id='${id}']`).val(computed_price)

            var pointsPerServ = parseInt(points) / parseInt(quantity)
            var computed_points = parseInt(quantity) * parseInt(pointsPerServ)
            $(`.newPointsEq[id='${id}']`).val(computed_points)
          }
          else if(parseInt(quantity) > parseInt(1))
          {
            swal({
              title: "How many "+name+" will pay using points?",
              content: {
              element: "input",
              attributes: {
                type: "number",
                max: quantity,
              },
            },
            }).then(function(value){
              if(parseInt(value) > parseInt(quantity))
              {
                swal({
                  title: "ERROR",
                  text: "The inputed number is invalid please try again",
                  icon: 'error'
                }).then(function(){
                  e.target.checked=false
                })
              }
              else
              {
                var pricePerServ = parseInt(price) / parseInt(quantity)
                var computed_price = parseInt(value) * parseInt(pricePerServ)
                $(`.newhalaga[id='${id}']`).val(computed_price)

                var pointsPerServ = parseInt(points) / parseInt(quantity)
                var computed_points = parseInt(value) * parseInt(pointsPerServ)
                $(`.newPointsEq[id='${id}']`).val(computed_points)
                
              }

              if(value=='' || value==null)
              {
                 e.target.checked=false
              }
            })
          }
              setTimeout(calculate,3000)
        }
        else
        {
          var id = $(this).val()
          var newPointsEq = $(`.newPointsEq[id='${id}']`).val()
          var newPoints = $('.newPoints').val()
          var newhalaga = $(`.newhalaga[id='${id}'`).val()
