    script.
      $(document).ready(function() {
        var type = window.location.hash.substr(1);
        if(type == "notfound")
        {
          swal({
              title:'Oooopsss', 
              text:'User is not found', 
              icon:'warning'
              }).then(function(){
                location.href='/frontdesk'
              })
        }
        else if(type=="incorrect")
        {
          swal({
              title:'Oooopsss', 
              text:'Incorrect Password or Username', 
              icon:'warning'
              })
        }
      })
   


